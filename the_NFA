digraph {
	fake [style=invisible]
	q0 [root=true]
	q1 [shape=doublecircle]
	q2
	q3 [shape=doublecircle]
	q5
	q6 [shape=doublecircle]
	q4
		fake -> q0 [style=bold]
		q0 -> q1 [label=a]
		q1 -> q1 [label=b]
		q1 -> q2 [label="&"]
		q2 -> q3 [label=a]
		q2 -> q5 [label=b]
		q3 -> q2 [label=a]
		q3 -> q4 [label=b]
		q5 -> q6 [label=a]
		q6 -> q1 [label=b]
}